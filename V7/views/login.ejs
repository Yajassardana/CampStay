<%- include("./partials/header") %>
<div class="container" style="width:60%; max-width:500px; margin:0 auto;">
  <h1 style="text-align:center">User Log In</h1>
  <form action="/login" method="post">
    <div class="form-group">
      <input class="form-control" type="text" name="username" placeholder="Username">
    </div>
    <div class="form-group">
      <input class="form-control" type="password" name="password" placeholder="Password">
      </div>
      <div class="form-group">
        <button class="btn btn-md btn-block btn-primary">Log In</button>
        </div>
  </form>
  <a href="/campgrounds" class="text-primary">Go Back</a>
</div>
<%- include("./partials/footer") %>
